{
  "extends": "astro/tsconfigs/base",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@interface/*": [
        "src/interface/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@models/*": [
        "src/models/*"
      ],
      "@database/*": [
        "src/database/*"
      ]
    },
    "jsx": "preserve"
  }
}